{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": "airbnb-base",
  "rules": {
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "comma-dangle": "off",
    "arrow-parens": "off",
    "no-param-reassign": [2, { "props": false }],
    "max-len": ["error", { "code": 120 }]
  }
}
